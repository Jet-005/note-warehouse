		{"id":"b682bcb8216a79d1","type":"text","text":"关系演算语言：元组关系演算、域关系演算","x":840,"y":2460,"width":250,"height":60},
		{"id":"f58981528fa6b0f4","type":"text","text":"关系数据模型","x":-1280,"y":1003,"width":250,"height":60},
		{"id":"e0b84899ae067a9d","type":"text","text":"关系数据模型","x":-910,"y":827,"width":250,"height":60},
